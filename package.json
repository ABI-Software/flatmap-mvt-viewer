{
  "name": "flatmap-mvt-viewer",
  "version": "0.8.7",
  "description": "Flatmap viewer using Mapbox GL",
  "repository": "https://github.com/dbrnz/flatmap-mvt-viewer.git",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel index.html --port 1111",
    "build": "parcel build --no-cache --public-url . --no-source-maps index.html",
    "package": "parcel build src/flatmap-viewer.js --target node --bundle-node-modules --no-source-maps --no-cache",
    "docs": "./node_modules/.bin/esdoc -c .esdoc.json"
  },
  "author": "David Brooks",
  "license": "MIT",
  "dependencies": {
    "@turf/area": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "broadcast-channel": "^2.1.12",
    "dat.gui": "^0.7.6",
    "dialog-polyfill": "^0.5.0",
    "mapbox-gl": "^1.0.0",
    "n3": "^1.1.1",
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-clean-dist": "0.0.6"
  }
}
